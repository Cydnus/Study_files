@model IEnumerable<SemiProject.Controllers.Views.OSOR>

@{
	ViewBag.Title = "Index";
}
<nav>
	</br>
	<h2 style="display:inline">본사</h2>

	<div style="display:inline" align="right">
		<h4 style="display:inline"><a href=@{
		string temp = $"{ViewData["next1"]}";
		WriteTo(Output, temp);
	}>전체 매장 재고 보기</a>
</h4>
		<h4 style="display:inline">

			<span style="color:greenyellow">전체 매장 매출 보기</span>
		</h4>
		<h4 style="display:inline">
			<a href=@{
		string temp2 = $"{ViewData["next3"]}";
		WriteTo(Output, temp2);
	}>전체 직원 보기</a>
		</h4>
	</div>

	</br>
</nav>

<table class="table">
	<tr>
		<th> 지점명 </th>
		<th> 구매/환불일       </th>
		<th> 번호       </th>
		<th>상품명        </th>
		<th>개수        </th>
		<th>개당 가격        </th>
		<th>총 가격        </th>
		<th>결제수단      </th>
		<th>연령층      </th>
	</tr>

	@foreach (var item in Model)
	{
<tr>
	<td>
		@Html.DisplayFor(modelItem => item.localName)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.SRDate)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.TradeNo)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.ProductName)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Count)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Price)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Total)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Payment)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.AgeOfCustomer)
	</td>

</tr>
	}
	<tr>
		<td colspan="7" align="right"><h4>합계 : </h4></td>
		<td>
			<h4>
				@{
					int count = 0;
					foreach (var item in Model)
					{
						count += item.Total;
					}
					Write(count.ToString() + "원");
				}
			</h4>
		</td>
	</tr>

</table>

</table>
